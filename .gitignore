# Ignore node_modules and vendor folders
/node_modules
/vendor

# Ignore compiled Blade files
/storage/framework/views/*.php

# Ignore environment files
.env
.env.*.backup

# Ignore IDE and system-specific files
.idea
.vscode
.DS_Store
Thumbs.db

# Ignore Laravel storage and cache directories
/storage/*.key
/storage/*.log
/storage/*.backup
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/testing/*
/storage/framework/views/*
/storage/logs/*

# Ignore public storage symlink if present
/public/storage

# Ignore compiled assets
/public/hot
/public/storage
/public/mix-manifest.json

# Ignore Laravel Mix manifest and compiled files
/npm-debug.log*
/yarn-error.log*
/yarn-debug.log*
/public/js/app.js
/public/css/app.css
/mix-manifest.json

# Ignore PHPUnit result cache
.phpunit.result.cache

# Ignore backup files
*.bak
*.backup

# Ignore Composer related files
composer.lock
